

@model ASFront.ModelsView.TurnoverAppSumViewModels





<div class="row mt">
    <div class="col-md-12">
        <div class="content-panel">
            <h4> Ապրանքանյութական Պաշարներ</h4>
            <hr>

            <br />
@if (Model.TotalRemaining != "0")
{
            <table class="table table-striped table-advance table-hover">
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.TotalSalesPerMonth)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.TotalEarnings)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.TotalRemaining)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.TotalMoneyBalance)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.AverageWeightedUpward)
                    </th>


                </tr>

                <tr>
                    <td>
                        @Html.DisplayFor(model => model.TotalSalesPerMonth)
                    </td>
                    <td>
                        @Html.DisplayFor(model => model.TotalEarnings)
                    </td>
                    <td>
                        @Html.DisplayFor(model => model.TotalRemaining)
                    </td>
                    <td>
                        @Html.DisplayFor(model => model.TotalMoneyBalance)
                    </td>
                    <td>
                        @Html.DisplayFor(model => model.AverageWeightedUpward)
                    </td>


                </tr>

            </table>
}
else
{
    <h3> <span class="btn btn-block btn-warning no-btn"> Դեռևս մուտքագրված չէ ապրանքանյութական պաշարների տվյալներ: </span></h3>
}
        </div>
    </div>
</div>