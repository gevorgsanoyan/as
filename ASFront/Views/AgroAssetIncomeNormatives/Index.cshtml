@model ASFront.ModelsView.AgroAssetIncomeNormativesIntroGroupViewModel

@{
    ViewBag.Title = "Գյուղատնտեսական Եկամտի Նորմատիվներ";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Գյուղատնտեսական Եկամտի/Ծախսի Նորմատիվ</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-inline">
        <div class="SectionData row">
            @*<h4>Գյուղատնտեսական Եկամտի Նորմատիվներ</h4>*@
            <hr />
            @Html.ValidationSummary(true, "", new { @class = "text-danger" })
            <div class="form-group formMargin col-md-4">
                @Html.LabelFor(model => model.BrancheId, htmlAttributes: new { @class = "control-label col-md-12 left-label" })
             
                    @Html.DropDownList("BrancheId", null, Resources.PlaceHolders.Select, htmlAttributes: new { @class = "form-control" })
              
                <div class="col-md-12">
                    @Html.ValidationMessageFor(model => model.BrancheId, "", new { @class = "text-danger" })
                </div>
            </div>

            <div class="form-group formMargin col-md-4">
               
                    @Html.LabelFor(model => model.AgroAssetTypesId, htmlAttributes: new { @class = "control-label col-md-12 left-label" })
             
                <div class="col-md-12">
                    @Html.DropDownList("AgroAssetTypesId", null, Resources.PlaceHolders.Select, htmlAttributes: new { @class = "form-control" })

                </div>
                <div class="col-md-12">



                    @Html.ValidationMessageFor(model => model.AgroAssetTypesId, "", new { @class = "text-danger" })
                </div>
            </div>

            <div class="form-group formMargin col-md-4">
                <label class="control-label col-md-12 left-label" for="Create" style="color:transparent;">Խմբագրել / Ստեղծել</label>
                <div class="col-md-12">
                    <input type="submit" value="Խմբագրել / Ստեղծել" name="Create" class="btn btn-default" />
                </div>
            </div>


        </div>
    </div>
}



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}


