@model IEnumerable<ASFront.Models.Guarantors>

@{
    ViewBag.Title = "Երաշխավոր";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Երաշխավոր</h2>

<p>
    @Html.ActionLink(Resources.Page.CreateNew, "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.applications.userId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.clients.clientName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.applications.userId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.clients.clientName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
