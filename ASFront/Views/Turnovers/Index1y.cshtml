@model IEnumerable<ASFront.Models.Turnovers>

@{
    ViewBag.Title = "Ապրանքանյութական Պաշարներ";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Ապրանքանյութական Պաշարներ</h2>

<p>
    @Html.ActionLink(Resources.Page.CreateNew, "Create", new { ApplicationID = ViewBag.ApplicationID }, null)
</p>
<table class="table">
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.MeasurementUnits.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.productName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MonthlySalesQuantity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.COGS)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SalesPrice)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OutstandingQuantity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.note1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.note2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.note3)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
       
        <td>
            @Html.DisplayFor(modelItem => item.MeasurementUnits.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.productName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MonthlySalesQuantity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.COGS)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SalesPrice)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OutstandingQuantity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.note1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.note2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.note3)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id })
        </td>
    </tr>
}

</table>
