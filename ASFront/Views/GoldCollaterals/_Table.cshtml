@model IEnumerable<ASFront.Models.GoldCollaterals>




<table class="table table-striped table-advance table-hover table-Full">
    <tr>


        <th>
            @Html.DisplayNameFor(model => model.GoldTypes.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Quantity)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.TotalWeightWithJewels)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NetWeight)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PresenceOfAssayStamp)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GoldAssayes.Assay)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EstimatedUnitCostAMD)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EstimatedTotalCostAMD)
        </th>

        <th></th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>


        <td>
            @Html.DisplayFor(modelItem => item.GoldTypes.Title)
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Quantity)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.TotalWeightWithJewels)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NetWeight)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PresenceOfAssayStamp)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.GoldAssayes.Assay)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EstimatedUnitCostAMD)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EstimatedTotalCostAMD)
        </td>




        <td>
            <a href='@Url.Action("Edit",  new { id=item.Id })'
               data-toggle="popover" data-trigger="hover" data-content="Խմբագրել">
                <img src='@Url.Content("~/img/edit.gif")' />
            </a>

        </td>
        <td>
            <a href='@Url.Action("Copy",  new { id=item.Id })'
               data-toggle="popover" data-trigger="hover" data-content="Պատճեն">
                <img src='@Url.Content("~/img/copy.png")' />
            </a>
        </td>
    </tr>
    }

</table>


<script type="text/javascript">


    $(document).ready(function () {
        $('[data-toggle="popover"]').popover();
    });


</script>
