@using PagedList.Mvc;
@model PagedList.IPagedList<ASFront.Models.AppTypes>

@{
    ViewBag.Title = "Հաստատման տեսակ";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Հաստատման տեսակ</h2>

<p>
    @Html.ActionLink(Resources.Page.CreateNew, "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.First().appType)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.appType)
            </td>
            <td>
                @Html.ActionLink(Resources.Page.Edit, "Edit", new { id = item.AppTypesId })
            </td>
        </tr>
    }

</table>
<br />


@Html.Partial("_Paging", Model)
